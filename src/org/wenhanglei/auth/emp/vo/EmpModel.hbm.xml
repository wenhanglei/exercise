<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.wenhanglei.auth.emp.vo.EmpModel" table="tbl_emp" >

    <id name="uuid">
      <generator class="native"/>
    </id>
    <property name="name"/>
    <property name="realName"/>
    <property name="telephone"/>
    <property name="sex"/>
    <property name="birthday"/>
    <property name="email"/>
    <property name="address"/>
    <property name="loginTime"/>
    <property name="pwd"/>
    <many-to-one name="dept" column="deptId" class="org.wenhanglei.auth.dept.vo.DeptModel"></many-to-one>
  </class>
</hibernate-mapping>