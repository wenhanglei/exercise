<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.wenhanglei.auth.dept.vo.DeptModel" table="tbl_dept" >

    <id name="uuid">
      <generator class="native"/>
    </id>
    <property name="name"/>
    <property name="telephone"/>
    <set name="empList" table="tbl_emp">
      <key column="deptId"></key>
      <one-to-many class="org.wenhanglei.auth.emp.vo.EmpModel"/>
    </set>
  </class>
</hibernate-mapping>